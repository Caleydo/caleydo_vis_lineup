{
  "name": "caleydo_vis_lineup",
  "description": "A visualization plugin for tables using LineUp.js",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/caleydo_vis_lineup",
  "homepage": "http://www.caleydo.org/tools/lineup/",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*",
    "caleydo_d3": "*"
  },
  "caleydo": {
    "type": "plugin",
    "category": "vis",
    "plugins": {
      "web": [
        {
          "type": "vis",
          "id": "caleydo-vis-lineup",
          "name": "LineUp",
          "sizeDependsOnDataDimension": [
            false,
            true
          ],
          "filter": "table"
        }
      ]
    },
    "dependencies": {
      "web": {
        "lineupjs": "https://github.com/Caleydo/lineup.js.git"
      }
    },
    "requirejs-config": {
      "shim": {
        "lineupjs": [
          "css!${baseUrl}/lineupjs/dist/style"
        ]
      }
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
