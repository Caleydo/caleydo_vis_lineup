{
  "name": "caleydo_vis_lineup",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/caleydo_vis_lineup",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*",
    "caleydo_d3": "*"
  },
  "caleydo": {
    "plugins": {
      "web": [
        {
          "type": "vis",
          "id": "caleydo-vis-lineup",
          "name": "LineUp",
          "sizeDependsOnDataDimension": [
            false,
            true
          ],
          "filter": "table"
        }
      ]
    },
    "dependencies": {
      "web": {
        "lineupjsN": "https://github.com/sgratzl/lineup.js.git"
      }
    },
    "requirejs-config": {
      "shim": {
        "lineupjsN": [
          "css!${baseUrl}/lineupjsN/dist/style"
        ]
      }
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
