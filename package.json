{
  "name": "caleydo_vis_lineup",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/caleydo_vis_lineup",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*"
  },
  "caleydo": {
    "plugins": {
      "web": [
        {
          "type": "vis",
          "id": "caleydo-vis-lineup",
          "name": "LineUp",
          "sizeDependsOnDataDimension": [
            false,
            true
          ],
          "filter": "table"
        }
      ]
    },
    "dependencies": {
      "web": {
        "d3": "~3.5.5",
        "lineupjs": "https://github.com/Caleydo/lineup.js.git"
      }
    },
    "requirejs-config": {
      "shim": {
        "lineupjs": [
          "css!${baseUrl}/lineupjs/css/style"
        ]
      }
    }
  },
  "publishConfig": {
    "registry": "http://dev.caleydo.org/nexus/content/repositories/caleydo-web/"
  }
}
